    "react-dom": "^18",
    "react-i18next": "^13.5.0",
    "server-only": "^0.0.1",
    "zod": "^3.22.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@magic-ext/oauth": "^7.6.0",
    "@magic-sdk/admin": "1.8.0",
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-playwright": "^0.20.0",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^50.0.1",
    "happy-dom": "^12.10.3",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "msw": "^2.0.11",
    "postcss": "^8",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "prisma": "^5.7.1",
    "tailwind-merge": "^2.1.0",
    "tailwindcss": "^3.3.7",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "^1.1.0"
  },
  "name": "earlynode-nextjs-template",
  "private": true,
  "scripts": {
    "build": "next build",
    "start-web": "next dev",
    "dev": "docker-compose up -d && npm run start-web",
    "format": "prettier --write .",
    "lint": "next lint",
    "start": "next start",
    "type-check": "tsc -b",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:e2e": "npx playwright test",
    "test:e2e:open": "npx playwright test --ui",
    "prisma:deploy": "npx prisma migrate deploy && npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name",
    "prisma:push": "npx prisma db push && npx prisma generate",
    "prisma:reset-dev": "run-s prisma:wipe prisma:seed dev",
    "prisma:reset-dev-with-mocks": "run-s prisma:wipe prisma:seed dev-with-mocks",
    "prisma:seed": "tsx ./prisma/seed.ts",
    "prisma:setup": "prisma generate && prisma migrate deploy && prisma db push",
    "prisma:studio": "npx prisma studio",
    "prepare": "husky install"
  },
  "version": "0.1.0"
}
